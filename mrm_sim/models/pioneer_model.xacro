<?xml version="1.0"?>
<sdf version='1.4' xmlns:sensor="http://playerstage.sourceforge.net/gazebo/xmlschema/#sensor"
			       xmlns:controller="http://playerstage.sourceforge.net/gazebo/xmlschema/#controller"
			       xmlns:interface="http://playerstage.sourceforge.net/gazebo/xmlschema/#interface"
			       xmlns:xacro="http://playerstage.sourceforge.net/gazebo/xmlschema/#interface">

  <world name='default'>
    <model name='ground_plane'>
      <static>1</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>100</mu>
                <mu2>50</mu2>
              </ode>
            </friction>
            <bounce/>
            <contact>
              <ode/>
            </contact>
          </surface>
          <max_contacts>10</max_contacts>
        </collision>
        <visual name='visual'>
          <cast_shadows>0</cast_shadows>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <material>
            <script>
              <uri>file://media/materials/scripts/gazebo.material</uri>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
        <velocity_decay>
          <linear>0</linear>
          <angular>0</angular>
        </velocity_decay>
        <self_collide>0</self_collide>
        <kinematic>0</kinematic>
        <gravity>1</gravity>
      </link>
    </model>
    <light name='sun' type='directional'>
      <cast_shadows>1</cast_shadows>
      <pose>0 0 20 0 -0 0</pose>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>0.5 0.1 -0.9</direction>
    </light>
    <physics type='ode'>
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>1000</real_time_update_rate>
      <gravity>0 0 -9.8</gravity>
    </physics>
    <scene>
      <ambient>0.4 0.4 0.4 1</ambient>
      <background>0.7 0.7 0.7 1</background>
      <shadows>1</shadows>
    </scene>

    <xacro:macro name="pioneer_model" params="robot_id position">

	    <model name='pioneer3at_${robot_id}'>

    	  <pose>${position}</pose>
	      <!-- Shaft #1 -->
	      <!-- <pose>8.5 -0.35 0.75 0 0 3.1415</pose> -->

	      <!-- Tunnel #1 -->
	      <!-- <pose>6.9 -0.35 0.65 0 0 3.1415</pose> -->

	      <!-- Shaft #2 -->
	      <!-- <pose>-1.322932 -0.327292 0.479293 0 0 3.1415</pose> -->

	      <!-- Shaft #3 -->
	      <!-- <pose>-8.26 -0.33 0.75 0 0 0</pose> -->
	      <static>0</static>
	      <link name='chassis'>
	        <inertial>
	          <mass>500</mass>
	          <inertia>
	            <ixx>0.3338</ixx>
	            <ixy>0</ixy>
	            <ixz>0</ixz>
	            <iyy>0.4783</iyy>
	            <iyz>0</iyz>
	            <izz>0.3338</izz>
	          </inertia>
	        </inertial>
	        <collision name='collision'>
	          <geometry>
	            <box>
	              <size>0.501 0.2 0.19</size>
	            </box>
	          </geometry>
	          <max_contacts>10</max_contacts>
	          <surface>
	            <bounce/>
	            <friction>
	              <ode/>
	            </friction>
	            <contact>
	              <ode/>
	            </contact>
	          </surface>
	        </collision>
	        <visual name='visual'>
	          <geometry>
	            <mesh>
	              <uri>$(find mrm_sim)/meshes/chassis.dae</uri>
	            </mesh>
	          </geometry>
	        </visual>
	        <velocity_decay>
	          <linear>0</linear>
	          <angular>0</angular>
	        </velocity_decay>
	        <self_collide>0</self_collide>
	        <kinematic>0</kinematic>
	        <gravity>1</gravity>
	      </link>
	      <link name='right_front_wheel'>
	        <pose>0.125 -0.1005 -0.06 1.5707 -0 0</pose>
	        <collision name='collision'>
	          <geometry>
	            <cylinder>
	              <radius>0.11</radius>
	              <length>0.075</length>
	            </cylinder>
	          </geometry>
	          <surface>
	            <friction>
	              <ode>
	                <mu>1</mu>
	                <mu2>1</mu2>
	                <slip1>0.5</slip1>
	                <slip2>0</slip2>
	              </ode>
	            </friction>
	            <bounce/>
	            <contact>
	              <ode/>
	            </contact>
	          </surface>
	          <max_contacts>10</max_contacts>
	        </collision>
	        <visual name='visual'>
	          <geometry>
	            <mesh>
	              <uri>$(find mrm_sim)/meshes/wheel.dae</uri>
	            </mesh>
	          </geometry>
	        </visual>
	        <velocity_decay>
	          <linear>0</linear>
	          <angular>0</angular>
	        </velocity_decay>
	        <self_collide>0</self_collide>
	        <inertial>
	          <inertia>
	            <ixx>1</ixx>
	            <ixy>0</ixy>
	            <ixz>0</ixz>
	            <iyy>1</iyy>
	            <iyz>0</iyz>
	            <izz>1</izz>
	          </inertia>
	          <mass>1</mass>
	        </inertial>
	        <kinematic>0</kinematic>
	        <gravity>1</gravity>
	      </link>
	      <link name='left_front_wheel'>
	        <pose>0.125 0.1005 -0.06 1.5707 -0 0</pose>
	        <collision name='collision'>
	          <geometry>
	            <cylinder>
	              <radius>0.11</radius>
	              <length>0.075</length>
	            </cylinder>
	          </geometry>
	          <surface>
	            <friction>
	              <ode>
	                <mu>1</mu>
	                <mu2>1</mu2>
	                <slip1>0.5</slip1>
	                <slip2>0</slip2>
	              </ode>
	            </friction>
	            <bounce/>
	            <contact>
	              <ode/>
	            </contact>
	          </surface>
	          <max_contacts>10</max_contacts>
	        </collision>
	        <visual name='visual'>
	          <geometry>
	            <mesh>
	              <uri>$(find mrm_sim)/meshes/wheel.dae</uri>
	            </mesh>
	          </geometry>
	        </visual>
	        <velocity_decay>
	          <linear>0</linear>
	          <angular>0</angular>
	        </velocity_decay>
	        <self_collide>0</self_collide>
	        <inertial>
	          <inertia>
	            <ixx>1</ixx>
	            <ixy>0</ixy>
	            <ixz>0</ixz>
	            <iyy>1</iyy>
	            <iyz>0</iyz>
	            <izz>1</izz>
	          </inertia>
	          <mass>1</mass>
	        </inertial>
	        <kinematic>0</kinematic>
	        <gravity>1</gravity>
	      </link>
	      <link name='right_rear_wheel'>
	        <pose>-0.125 -0.1005 -0.06 1.5707 -0 0</pose>
	        <collision name='collision'>
	          <geometry>
	            <cylinder>
	              <radius>0.11</radius>
	              <length>0.075</length>
	            </cylinder>
	          </geometry>
	          <surface>
	            <friction>
	              <ode>
	                <mu>1</mu>
	                <mu2>1</mu2>
	                <slip1>0.5</slip1>
	                <slip2>0</slip2>
	              </ode>
	            </friction>
	            <bounce/>
	            <contact>
	              <ode/>
	            </contact>
	          </surface>
	          <max_contacts>10</max_contacts>
	        </collision>
	        <visual name='visual'>
	          <geometry>
	            <mesh>
	              <uri>$(find mrm_sim)/meshes/wheel.dae</uri>
	            </mesh>
	          </geometry>
	        </visual>
	        <velocity_decay>
	          <linear>0</linear>
	          <angular>0</angular>
	        </velocity_decay>
	        <self_collide>0</self_collide>
	        <inertial>
	          <inertia>
	            <ixx>1</ixx>
	            <ixy>0</ixy>
	            <ixz>0</ixz>
	            <iyy>1</iyy>
	            <iyz>0</iyz>
	            <izz>1</izz>
	          </inertia>
	          <mass>1</mass>
	        </inertial>
	        <kinematic>0</kinematic>
	        <gravity>1</gravity>
	      </link>
	      <link name='left_rear_wheel'>
	        <pose>-0.125 0.1005 -0.06 1.5707 -0 0</pose>
	        <collision name='collision'>
	          <geometry>
	            <cylinder>
	              <radius>0.11</radius>
	              <length>0.075</length>
	            </cylinder>
	          </geometry>
	          <surface>
	            <friction>
	              <ode>
	                <mu>1</mu>
	                <mu2>1</mu2>
	                <slip1>0.5</slip1>
	                <slip2>0</slip2>
	              </ode>
	            </friction>
	            <bounce/>
	            <contact>
	              <ode/>
	            </contact>
	          </surface>
	          <max_contacts>10</max_contacts>
	        </collision>
	        <visual name='visual'>
	          <geometry>
	            <mesh>
	              <scale>1 1 1</scale>
	              <uri>$(find mrm_sim)/meshes/wheel.dae</uri>
	            </mesh>
	          </geometry>
	        </visual>
	        <velocity_decay>
	          <linear>0</linear>
	          <angular>0</angular>
	        </velocity_decay>
	        <self_collide>0</self_collide>
	        <inertial>
	          <inertia>
	            <ixx>1</ixx>
	            <ixy>0</ixy>
	            <ixz>0</ixz>
	            <iyy>1</iyy>
	            <iyz>0</iyz>
	            <izz>1</izz>
	          </inertia>
	          <mass>1</mass>
	        </inertial>
	        <kinematic>0</kinematic>
	        <gravity>1</gravity>
	      </link>
	      <joint name='right_front' type='revolute'>
	        <pose>0 0 0 0 -0 0</pose>
	        <child>right_front_wheel</child>
	        <parent>chassis</parent>
	        <axis>
	          <xyz>0 1 0</xyz>
	          <limit>
	            <lower>-1e+16</lower>
	            <upper>1e+16</upper>
	          </limit>
	        </axis>
	      </joint>
	      <joint name='right_rear' type='revolute'>
	        <pose>0 0 0 0 -0 0</pose>
	        <child>right_rear_wheel</child>
	        <parent>chassis</parent>
	        <axis>
	          <xyz>0 1 0</xyz>
	          <limit>
	            <lower>-1e+16</lower>
	            <upper>1e+16</upper>
	          </limit>
	        </axis>
	      </joint>
	      <joint name='left_front' type='revolute'>
	        <pose>0 0 0 0 -0 0</pose>
	        <child>left_front_wheel</child>
	        <parent>chassis</parent>
	        <axis>
	          <xyz>0 1 0</xyz>
	          <limit>
	            <lower>-1e+16</lower>
	            <upper>1e+16</upper>
	          </limit>
	        </axis>
	      </joint>
	      <joint name='left_rear' type='revolute'>
	        <pose>0 0 0 0 -0 0</pose>
	        <child>left_rear_wheel</child>
	        <parent>chassis</parent>
	        <axis>
	          <xyz>0 1 0</xyz>
	          <limit>
	            <lower>-1e+16</lower>
	            <upper>1e+16</upper>
	          </limit>
	        </axis>
	      </joint>
	      <link name='hokuyo::link'>
	        <inertial>
	          <mass>0.1</mass>
	          <inertia>
	            <ixx>1</ixx>
	            <ixy>0</ixy>
	            <ixz>0</ixz>
	            <iyy>1</iyy>
	            <iyz>0</iyz>
	            <izz>1</izz>
	          </inertia>
	        </inertial>
	        <visual name='visual'>
	          <geometry>
	            <mesh filename="package://mrm_sim/meshes/hokuyo.dae" />
	          </geometry>
	        </visual>
	        <collision name='collision-base'>
	          <pose>0 0 -0.0145 0 -0 0</pose>
	          <geometry>
	            <box>
	              <size>0.05 0.05 0.041</size>
	            </box>
	          </geometry>
	          <max_contacts>10</max_contacts>
	          <surface>
	            <bounce/>
	            <friction>
	              <ode/>
	            </friction>
	            <contact>
	              <ode/>
	            </contact>
	          </surface>
	        </collision>
	        <collision name='collision-top'>
	          <pose>0 0 0.0205 0 -0 0</pose>
	          <geometry>
	            <cylinder>
	              <radius>0.021</radius>
	              <length>0.029</length>
	            </cylinder>
	          </geometry>
	          <max_contacts>10</max_contacts>
	          <surface>
	            <bounce/>
	            <friction>
	              <ode/>
	            </friction>
	            <contact>
	              <ode/>
	            </contact>
	          </surface>
	        </collision>
	        <sensor name='laser' type='ray'>
	          <pose>0 0 0 0 -0 0</pose>
	          <ray>
	            <scan>
	              <horizontal>
	                <samples>1040</samples>
	                <resolution>1</resolution>
	                <min_angle>-2.26889</min_angle>
	                <max_angle>2.2689</max_angle>
	              </horizontal>
	            </scan>
	            <range>
	              <min>0.1</min>
	              <max>30</max>
	              <resolution>0.01</resolution>
	            </range>
	          </ray>
	          <!-- <plugin name='laser' filename='libRayPlugin.so'/> -->
	          <plugin name='gazebo_ros_hokuyo_laser_controller' filename='libgazebo_ros_laser.so'>
	            <gaussianNoise>0.01</gaussianNoise>
	            <alwaysOn>true</alwaysOn>
	            <hokuyoMinIntensity>101</hokuyoMinIntensity>
	            <updateRate>40.0</updateRate>
	            <topicName>/pioneer_${robot_id}/scan_raw</topicName>
	            <frameName>/pioneer_${robot_id}/odom_tf__not_used__a</frameName>
	            <interface:laser/>
	          </plugin>
	          <always_on>1</always_on>
	          <update_rate>30</update_rate>
	          <visualize>1</visualize>
	        </sensor>
	        <velocity_decay>
	          <linear>0</linear>
	          <angular>0</angular>
	        </velocity_decay>
	        <pose>$(arg laser_forward_offset) 0 0.06 0 -0 0</pose>
	        <self_collide>0</self_collide>
	        <kinematic>0</kinematic>
	        <gravity>1</gravity>
	      </link>
	      <joint name='hokuyo_joint' type='revolute'>
	        <child>hokuyo::link</child>
	        <parent>chassis</parent>
	        <axis>
	          <xyz>0 0 1</xyz>
	          <limit>
	            <upper>0</upper>
	            <lower>0</lower>
	          </limit>
	        </axis>
	      </joint>

		  
		    <link name="camera::link">
		      <pose>0.10 0.05 0.15 0 0 0</pose>
		      <inertial>
		        <mass>0.1</mass>
		      </inertial>
		      <collision name="collision">
		        <geometry>
		          <box>
		            <size>0.1 0.1 0.1</size>
		          </box>
		        </geometry>
		      </collision>
		      <visual name="visual">
		        <geometry>
		          <box>
		            <size>0.1 0.1 0.1</size>
		          </box>
		        </geometry>
		      </visual>
		      <sensor name="camera" type="camera">
		        <camera>
		          <horizontal_fov>0.752</horizontal_fov>
		          <image>
		            <width>640</width>
		            <height>480</height>
		          </image>
		          <clip>
		            <near>0.1</near>
		            <far>100</far>
		          </clip>
		        </camera>
		        <always_on>1</always_on>
		        <update_rate>15</update_rate>
		        <visualize>false</visualize>

		        <plugin name='ros_camera' filename='libgazebo_ros_camera.so'>
	        		<robotNamespace>/pioneer_${robot_id}</robotNamespace>
		        </plugin>

		      </sensor>
		    </link>
	      <joint name="camera_joint" type="revolute">
	        <child>camera::link</child>
	        <parent>chassis</parent>
	        <axis>
	          <xyz>0 0 1</xyz>
	          <limit>
	            <upper>0</upper>
	            <lower>0</lower>
	          </limit>
	        </axis>
	      </joint>

	      <plugin name='skid_steer_drive_controller' filename='libgazebo_ros_skid_steer_drive.so'>
	        <updateRate>100.0</updateRate>
	        <robotNamespace>/pioneer_${robot_id}</robotNamespace>
	        <leftFrontJoint>left_front</leftFrontJoint>
	        <rightFrontJoint>right_front</rightFrontJoint>
	        <leftRearJoint>left_rear</leftRearJoint>
	        <rightRearJoint>right_rear</rightRearJoint>
	        <wheelSeparation>0.4</wheelSeparation>
	        <wheelDiameter>0.215</wheelDiameter>
	        <odometryTopic>odom_raw</odometryTopic>
	        <robotBaseFrame>/pioneer_${robot_id}/odom_tf</robotBaseFrame>
	        <torque>120</torque>
	        <commandTopic>cmd_vel</commandTopic>
	        <broadcastTF>1</broadcastTF>
	      </plugin>
<!-- 	      <plugin filename="libgazebo_ros_imu.so" name="imu_en_ros">
	  		  <bodyName>chassis</bodyName>
	  		  <robotNamespace>/pioneer_${robot_id}</robotNamespace>
	  		  <topicName>imu</topicName>
	  		  <gaussianNoise>0.00</gaussianNoise>
	  		  <updateRate>45.0</updateRate>
	  	  </plugin> -->
	    </model>

	</xacro:macro>

	<xacro:include filename="$(find mrm_sim)/models/$(arg robots_count)robots.xacro" />

	<model name='willowgarage'>
      <static>1</static>
      <pose>-11 -29 0 0 -0 0</pose>
      <link name='walls'>
        <collision name='collision'>
          <geometry>
            <mesh>
              <uri>$(find mrm_sim)/meshes/willowgarage_collision.dae</uri>
            </mesh>
          </geometry>
          <max_contacts>10</max_contacts>
          <surface>
            <bounce/>
            <friction>
              <ode/>
            </friction>
            <contact>
              <ode/>
            </contact>
          </surface>
        </collision>
        <visual name='visual'>
          <geometry>
            <mesh>
              <uri>$(find mrm_sim)/meshes/willowgarage_visual.dae</uri>
            </mesh>
          </geometry>
          <cast_shadows>0</cast_shadows>
        </visual>
        <velocity_decay>
          <linear>0</linear>
          <angular>0</angular>
        </velocity_decay>
        <self_collide>0</self_collide>
        <kinematic>0</kinematic>
        <gravity>1</gravity>
      </link>
    </model>

  </world>
</sdf>